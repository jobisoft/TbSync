<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>tzpush@capeproject.org.uk</em:id>
        <em:name>tzpush</em:name>
        <em:version>2.6</em:version>
        <em:description>Z-Sync to Thunderbird Addressbook</em:description>
        <em:creator>Mark N.</em:creator>
        <em:homepageURL>http://code.google.com/p/tz-push/</em:homepageURL>
        <em:iconURL>chrome://tzpush/skin/tz-push.png</em:iconURL>
        <em:type>2</em:type> <!-- type=extension --> 

        <em:targetApplication>
            <!-- SeaMonkey -->
           <Description>
               <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
               <em:minVersion>2.0</em:minVersion>
               <em:maxVersion>2.31.*</em:maxVersion>
           </Description>
           <!-- Thunderbird -->
           <Description>
                <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
                <em:minVersion>10.0</em:minVersion>
                <em:maxVersion>40.*</em:maxVersion>
           </Description>

        </em:targetApplication>
    </Description>
</RDF>
